
@{
    ViewBag.Title = "ArticleFormat";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}


@model TatliGezi.Models.MultipleData.ArticleAdd


<h4>Makale Tipi Seç: </h4>


@foreach (var item in Model.fList)
{
    <ul class="list-group list-group-flush">
        <li class="list-group-item list-group-item-action ">
            <a href="/Admin/Article/ArticleAdd/@item.ID" class="text-secondary">@item.FormatName</a>

        </li>
    </ul>

}

